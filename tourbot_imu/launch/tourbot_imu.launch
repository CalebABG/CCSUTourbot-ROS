<launch>
    <!--<node pkg="tourbot_imu" type="imu_node2.py" name="imu_node" respawn="true" output="screen"></node>-->
    <node pkg="tourbot_imu" type="imu_node1.py" name="imu_node" respawn="true" output="screen"/>
    <node pkg="tourbot_imu" type="imu_tf_pub.py" name="imu_tf_node" respawn="true" output="screen"/>

    <node pkg="imu_complementary_filter" type="complementary_filter_node"
          name="complementary_filter_gain_node" output="screen">

        <param name="do_bias_estimation" value="true"/>
        <param name="do_adaptive_gain" value="true"/>
        <param name="use_mag" value="false"/>
        <param name="gain_acc" value="0.01"/>
        <param name="gain_mag" value="0.01"/>
        <param name="publish_tf" value="false"/>
        <param name="reverse_tf" value="false"/>
        <!--<param name="fixed_frame" value="map"/>-->

    </node>

</launch>
