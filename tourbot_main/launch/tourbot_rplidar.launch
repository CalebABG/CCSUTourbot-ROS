<launch>
    <node name="rplidarNode" pkg="rplidar_ros" type="rplidarNode" output="screen">

        <param name="serial_port" type="string"
               value="/dev/serial/by-id/usb-Silicon_Labs_CP2102_USB_to_UART_Bridge_Controller_0001-if00-port0"/>
        <!--<param name="serial_port"       type="string"   value="/dev/ttyUSB0"/>-->
        <param name="serial_baudrate" type="int" value="115200"/><!--A1/A2 -->
        <param name="frame_id" type="string" value="laser"/>
        <param name="inverted" type="bool" value="false"/>
        <param name="angle_compensate" type="bool" value="true"/>

        <!--
        Scan modes:
        - Standard
        - Express
        - Boost
        - Stability
        -->
        <param name="scan_mode" type="string" value="Stability"/>
        <!--<param name="scan_mode"         type="string"   value="Boost"/>-->
        <!--<param name="motor_pwm"         type="int"      value="660"/>-->
        <!--<param name="motor_pwm"         type="int"      value="1000"/>-->
        <param name="motor_pwm" type="int" value="850"/>

        <!--<remap from="/scan" to="tourbot/rplidar_scan" />-->
    </node>
</launch>
